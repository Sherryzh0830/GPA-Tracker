          onChange={(course) => setcourseName(course.target.value)}\n          value={courseName}\n          placeholder=\"Please enter course name\"\n        />\n\n        <button\n          onClick={() => {\n            AddCourse();\n            document.getElementsByClassName(\"Add-Course\")[0].style.display =\n              \"block\";\n          }}\n        >\n          Submit\n        </button>\n      </div>\n\n      {show && (\n        <>\n          <hr />\n          <h3 className=\"courseName\">{display}</h3>\n\n          <table className=\"courseTable\">\n            <thead>\n              <tr>\n                <th>Component</th>\n                <th>Weight</th>\n                <th>Grade</th>\n                <th className=\"edit-col\">Edit</th>\n              </tr>\n            </thead>\n            <tbody>\n              {Class.map((val) => {\n                return (\n                  <>\n                    <tr>\n                      <td>{val.component}</td>\n                      <td>{val.weight}%</td>\n                      <td>{val.grade}</td>\n                      <td className=\"edit-col\">\n                        <div className=\"edit-buttons\">\n                          <button\n                            onClick={() => UpdateEvent(val._id)}\n                            style={{\n                              backgroundColor: \"#f4d35e\",\n                              borderColor: \"#faf0ca\",\n                              padding: \"0.4rem\",\n                              fontWeight: \"bold\",\n                              color: \"#0d3b66\",\n                            }}\n                          >\n                            Update\n                          </button>\n                          <button\n                            onClick={() => DeleteEvent(val._id)}\n                            style={{\n                              backgroundColor: \"#f4d35e\",\n                              borderColor: \"#faf0ca\",\n        